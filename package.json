{
  "name": "registro-ponto-icons",
  "version": "1.0.0",
  "license": "MIT",
  "files": [
    "icons",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "prepublishOnly": "npm run build",
    "prebuild": "rimraf ./icons ./react/icons ./optimized/icons",
    "build-icons": "rimraf ./icons && svgo --config=svgo.icons.mjs -f ./src/icons -o ./optimized/icons --pretty --indent=2",
    "build": "npm run prebuild && npm run build-icons && node ./scripts/build.js",
    "release-channel": "node ./scripts/release-channel.js",
    "release-notes": "node ./scripts/release-notes.js"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-react-jsx": "^7.12.12",
    "@svgr/core": "^5.5.0",
    "@vue/compiler-dom": "^3.0.5",
    "camelcase": "^6.0.0",
    "prettier": "^2.8.7",
    "rimraf": "^3.0.2",
    "svgo": "^3.0.2"
  }
}
